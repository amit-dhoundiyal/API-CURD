
<TABLE border=3>
<tr>
	<th> Date</th>
	<th>Description</th>
	<th>Type</th>
	<th>Amount</th>
</tr>
<% if @transactions != nil %>
	<% @transactions.each do |i| %>
		<tr>
		<td> <%= i.created_at.to_date %> </td>
		<td> <%= i.description %> </td>
		<td><%= i.typeof %></td>
		<td><%= i.amount %></td>
		</tr>
<% end %>
<% end %>
</TABLE>